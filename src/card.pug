    - var pageClass = 'category'
    - var menuHref = '/'

block detail
    section.detail
        .outer 
            .inner 
                .detail__container 
                    .detail__slider
                        div(style="position: relative;").detail__slider-js
                            .card__label
                                p._yellow Новинка
                            .detail__wrapper.swiper-wrapper 
                                .detail__slide.swiper-slide 
                                    img(src="./images/detail/img1.jpg" alt="Картинка")
                                .detail__slide.swiper-slide 
                                    img(src="./images/detail/img2.jpg" alt="Картинка")
                                .detail__slide.swiper-slide 
                                    img(src="./images/detail/img3.jpg" alt="Картинка")
                                .detail__slide.swiper-slide 
                                    img(src="./images/detail/img4.jpg" alt="Картинка")
                                .detail__slide.swiper-slide 
                                    img(src="./images/detail/img5.jpg" alt="Картинка")
                            .navigation__button.navigation__button-prev.detail__button-prev-js
                                <svg width="14" height="26" viewBox="0 0 16 31" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M15.2725 30.8029L16 30.0814L1.45441 15.645L16 1.20862L15.2725 0.487127L0 15.645L15.2725 30.8029Z"/>
                                </svg>
                            .navigation__button.navigation__button-next.detail__button-next-js
                                <svg width="14" height="26" viewBox="0 0 16 31" fill="none" xmlns="http://www.w3.org/2000/svg">
                                    <path d="M15.2725 30.8029L16 30.0814L1.45441 15.645L16 1.20862L15.2725 0.487127L0 15.645L15.2725 30.8029Z"/>
                                </svg>

                        .detail__thumbnail.detail__thumbnail-js
                            .swiper-wrapper 
                                .swiper-slide 
                                    img(src='./images/detail/img1.jpg')
                                .swiper-slide 
                                    img(src='./images/detail/img2.jpg')
                                .swiper-slide 
                                    img(src='./images/detail/img3.jpg')
                                .swiper-slide 
                                    img(src='./images/detail/img4.jpg')
                                .swiper-slide 
                                    img(src='./images/detail/img5.jpg')
                    .detail__info
                        nav.breadcrumbs
                            ul.breadcrumbs__list
                                li
                                    a(href="/") Главная
                                    span.breadcrumbs__divider \
                                li.is-active #{title}
                        h1.title Террасная доска из ДПК Deckart Universal Серый
                        p Есть в наличии
                        ul 
                            li
                                +icon('houses')
                                | Для дома и дачи
                            li
                                +icon('swimming-pool')
                                | Для бассейнов и пирсов
                        .detail__color 
                            p Цвет доски: 
                                span.color-text-js Серый
                            ul.color-tabs-js 
                                li(data-color="Цвет 1") 
                                    img(src='./images/color/1.png' alt="Цвет 1")
                                li(data-color="Цвет 2")  
                                    img(src='./images/color/2.png' alt="Цвет 2")
                                li(data-color="Цвет 3")  
                                    img(src='./images/color/3.png' alt="Цвет 3")
                                li(data-color="Серый")
                                    img(src='./images/color/4.png' alt="Серый")
                                li(data-color="Цвет 4") 
                                    img(src='./images/color/5.png' alt="Цвет 4")
                                li(data-color="Цвет 5")  
                                    img(src='./images/color/6.png' alt="Цвет 5")
                        .card__size.detail__size
                            p Длина доски: 
                            ul
                                li 3м
                                li._disabled 4м
                                li 5м
                                li._active 6м
                                li 8м
                            .detail__size-info
                                p Размер: 
                                    span 150х25 мм
                                p Вес: 
                                    span 116кг\м
                                        sup 2
                                p Производитель:
                                    span Deckart (Россия)
                        .detail__price 
                            .card__for 
                                p Цена за:
                                ul 
                                    li._active М
                                        sup 2
                                    li шт
                                    li п\м
                            .card__price
                                .card__price-new
                                    | 1 990
                                    div ₽
                                    div м
                                        sup 2
                                .card__price-old
                                    | 2 590
                                    div ₽
                                    div м
                                        sup 2
                            p Цена за пог. метр 1 590
                                span ₽
                        .detail__buttons 
                            .card__calc.calc-js
                                button.card__remove-js -
                                p.card__calc-num-js 1
                                button.card__add-js +           
                            .card__buttons 
                                +btn('Купить в 1 клик', 'buy')
                                button
                                    +icon('basket')
                                    div Добавить в корзину
                                    p Добавлено 
