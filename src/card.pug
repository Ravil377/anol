                        data-min="4000"
                        data-max="8500"
                        data-from="4000"
                        data-to="8500")
            .filter__color 
                p Цвет
                +checkbox('Коричневый/венге', 'color1')
                +checkbox('Светлосерый', 'color2')
                +checkbox('Желтый нарцисс', 'color3')
                +checkbox('Слоновая кость', 'color4')
            .filter__size 
                p Размер
                +select('140х20 мм', 'size')
            .filter__length
                p Длина
                +select('3 метра', 'length')
            .filter__made
                p Производитель
                +select('Deckart (Россия)', 'length')
            .filter__other 
                +checkbox('Распродажа', 'sale')
                +checkbox('Новинки', 'Новинки')         
